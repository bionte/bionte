<UserControl x:Name="UserControlView4Regions" x:Class="Bionte.Views.View4Regions"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel x:Name="MainPanel">
    	<DockPanel x:Name="Top" DockPanel.Dock="Top" VerticalAlignment="Top"/>
    	<DockPanel x:Name="Bottom" DockPanel.Dock="Bottom" VerticalAlignment="Bottom"/>
    	<DockPanel x:Name="Left" HorizontalAlignment="Left"/>
    	<DockPanel x:Name="Rigth" DockPanel.Dock="Right" HorizontalAlignment="Right"/>
    	<DockPanel x:Name="Center"/>
    </DockPanel>
</UserControl>
